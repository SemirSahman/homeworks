package ba.bitcamp.task1;

import java.util.Scanner;

public class Main {
	
	//Testing methods in main

	public static void main(String[] args) {
		

		CampStringBuilder csb = new CampStringBuilder(new char[]{'1', '2', 'S'});

		csb.append(Math.PI);
		System.out.println(csb.toString());
		csb.prepend(Math.E);
		System.out.println(csb.toString());
		
	}
}