package ba.bitcamp.task6;

public class Main {

	public static void main(String[] args) {
		
		Dog d = new Dog("Milo", 1, "Jack Russell Terrier");
		Cat c = new Cat("Tom", 2); 
		Pet[] p = new Pet[]{d, c};
		System.out.println(d); //Prints Av av
		System.out.println(c); //Prints Go away human
		for (int i = 0; i < p.length; i++) {
			System.out.println(p); //Runs toString method from class Pet, which returns "address", because it`s not overridden.
		}
		
	}
}